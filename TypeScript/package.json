{
  "name": "@chatfield/core",
  "version": "1.0.0a1",
  "description": "Conversational data collection powered by LLMs - TypeScript/JavaScript implementation",
  "browser": "dist/lean/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/node/types/index.d.ts",
      "node": {
        "import": "./dist/node/esm/index.js",
        "require": "./dist/node/cjs/index.js"
      },
      "browser": "./dist/standalone/esm/index.js"
    },
    "./lean": {
      "types": "./dist/node/types/index.d.ts",
      "import": "./chatfield/index.ts"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run build:node && npm run build:browser",
    "build:node": "npm run build:node:cjs && npm run build:node:esm && npm run build:node:types",
    "build:node:cjs": "tsc --module node16 --moduleResolution node16 --declaration false --outDir dist/node/cjs",
    "build:node:esm": "tsc --module esnext --moduleResolution bundler --declaration false --outDir dist/node/esm",
    "build:node:types": "tsc --declaration --emitDeclarationOnly --outDir dist/node/types",
    "build:browser": "npm run build:browser:lean && npm run build:browser:standalone",
    "build:browser:lean": "VARIANT=lean rollup --config rollup.config.mjs",
    "build:browser:standalone": "VARIANT=standalone rollup --config rollup.config.mjs",
    "build:browser:standalone:watch": "VARIANT=standalone rollup --config rollup.config.mjs --watch",
    "dev": "tsc --watch --outDir dist/node/cjs",
    "test": "jest --silent",
    "test:verbose": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint chatfield/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "proxy": "cd ../Python && python litellm_proxy.py",
    "examples": "http-server . -p 8080 -c-1"
  },
  "keywords": [
    "llm",
    "conversation",
    "data-collection",
    "ai",
    "chatgpt",
    "claude"
  ],
  "author": "Claudey With a Chance of DevOps",
  "license": "Apache-2.0",
  "dependencies": {
    "@langchain/core": "^0.3.78",
    "@langchain/langgraph": "^1.0.0-alpha.5",
    "@langchain/openai": "^0.6.14",
    "handlebars": "^4.7.8",
    "openai": "^4.70.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^8.0.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "rollup": "^4.52.3",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "ts-jest": "^29.4.1",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@langchain/core": "^0.3.78",
    "@langchain/openai": "^0.6.14",
    "handlebars": "^4.7.8",
    "openai": "^4.70.0",
    "react": "^18.0.0 || ^19.0.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "peerDependenciesMeta": {
    "@langchain/core": {
      "optional": true
    },
    "@langchain/langgraph": {
      "optional": true
    },
    "@langchain/openai": {
      "optional": true
    },
    "handlebars": {
      "optional": true
    },
    "openai": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "reflect-metadata": {
      "optional": true
    },
    "uuid": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "files": [
    "chatfield",
    "dist",
    "README.md",
    "LICENSE"
  ]
}
